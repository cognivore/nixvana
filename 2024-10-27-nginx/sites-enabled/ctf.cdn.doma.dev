server {
  server_name ctf.cdn.doma.dev;
  charset utf-8;

  location / {
    proxy_pass http://127.0.0.1:8354;
    proxy_max_temp_file_size 0;
  }

    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/ctf.cdn.doma.dev/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/ctf.cdn.doma.dev/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}
server {
    if ($host = ctf.cdn.doma.dev) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


  listen 80;
  server_name ctf.cdn.doma.dev;
    return 404; # managed by Certbot


}
